  "repository": {
    "type": "git",
    "url": "https://github.com/ken-rs/react-ssot-query.git"
  },
  "bugs": {
    "url": "https://github.com/ken-rs/react-ssot-query/issues"
  },
  "homepage": "https://github.com/ken-rs/react-ssot-query#readme",
  "keywords": [
    "react",
    "hooks",
    "query",
    "url",
    "ssot",
    "state"
  ],
  "author": "ken-rs",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-router-dom": ">=6.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.47",
    "@vitest/coverage-v8": "^3.1.4",
    "react-dom": "^18.2.0",
    "typescript": "^5.4.5",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  }
